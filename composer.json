{
	"name": "eipe/instagram",
	"type": "library",
	"description": "Replacement for cosenary/instagram that is a PHP Class for accessing Instagram's API.",
	"keywords": ["instagram", "api"],
	"homepage": "https://github.com/eipe/instagram",
	"license": "BSD",
	"authors": [
		{
			"name": "Christian Metz",
			"email": "christian-metz1@gmx.net",
			"homepage": "http://metzweb.net"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"ext-curl": "*"
	},
	"autoload": {
		"psr-4": {
			"MetzWeb\\Instagram\\": "src"
		}
	},
    "replace": {
      "cosenary/instagram":"2.3.*"
    },
	"extra": {
		"branch-alias": {
			"dev-master": "2.4-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
